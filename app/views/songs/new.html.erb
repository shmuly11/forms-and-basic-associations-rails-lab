<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
   <input id="song_notes_1", name="[notes][]">
  <input id="song_notes_2", name="[notes][]">
  <input id="song_notes_3", name="[notes][]">
  <br />

  <%= f.submit %>
<% end %>


